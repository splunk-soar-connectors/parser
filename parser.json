{
    "appid": "4004f489-1826-44a1-af12-9c74eea82eb0",
    "name": "Parser",
    "description": "This app extracts IOCs from various files such as PDFs or Emails",
    "type": "information",
    "product_vendor": "Phantom",
    "logo": "logo_phantom.svg",
    "logo_dark": "logo_phantom_dark.svg",
    "product_name": "Parser",
    "product_version_regex": ".*",
    "publisher": "Phantom",
    "license": "Copyright (c) Phantom Cyber Corporation, 2017-2018",
    "app_version": "1.0.10",
    "utctime_updated": "2018-02-13T18:18:28.000000Z",
    "package_name": "phantom_parser",
    "main_module": "parser_connector.pyc",
    "min_phantom_version": "3.0.251",
    "app_wizard_version": "1.0.0",
    "pip_dependencies": {
        "wheel": [
            {
                "module": "pdfminer",
                "input_file": "wheels/pdfminer-20140328-py2-none-any.whl"
            }
        ]
    },
    "configuration": {},
    "actions": [
        {
            "action": "extract ioc",
            "identifier": "parse_file",
            "description": "Create IOC artifacts from a file in the vault",
            "verbose": "If the <b>file_type</b> is left blank, the app will try to determine what type of file it is on its own. ",
            "type": "generic",
            "read_only": true,
            "parameters": {
                "vault_id": {
                    "description": "Vault ID",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "vault id"
                    ],
                    "default": "",
                    "order": 0
                },
                "label": {
                    "description": "Add container to this label",
                    "data_type": "string",
                    "required": false,
                    "primary": false,
                    "order": 1
                },
                "file_type": {
                    "description": "The type of file",
                    "data_type": "string",
                    "required": false,
                    "primary": false,
                    "value_list": [
                        "txt",
                        "email",
                        "pdf",
                        "docx",
                        "csv",
                        "html"
                    ],
                    "default": "",
                    "order": 2
                },
                "is_structured": {
                    "description": "Use first row of CSV file as field names",
                    "data_type": "boolean",
                    "required": true,
                    "primary": false,
                    "default": false,
                    "order": 3
                },
                "max_artifacts": {
                    "description": "Max number of artifacts",
                    "data_type": "numeric",
                    "required": false,
                    "primary": false,
                    "order": 4
                },
                "container_id": {
                    "description": "Add created artifacts to this container",
                    "data_type": "numeric",
                    "required": false,
                    "primary": false,
                    "order": 4
                }
            },
            "render": {
                "width": 12,
                "title": "Parse File",
                "type": "table",
                "height": 5
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string",
                    "example_values": [
                        "Container id: 52069, Artifacts found: 27",
                        "Container id: 52069, Artifacts found: 13"
                    ]
                },
                {
                    "data_path": "action_result.summary.container_id",
                    "data_type": "numeric",
                    "example_values": [
                        "52079",
                        "1776"
                    ],
                    "column_order": 0,
                    "column_name": "Container ID"
                },
                {
                    "data_path": "action_result.summary.artifacts_found",
                    "data_type": "numeric",
                    "example_values": [
                        7,
                        13
                    ]
                },
                {
                    "data_path": "action_result.parameter.label",
                    "data_type": "string",
                    "example_values": [
                        "Events"
                    ]
                },
                {
                    "data_path": "action_result.parameter.vault_id",
                    "data_type": "string",
                    "example_values": [
                        "0c4ad1e28e5e2e03f2f197e4e4b011c617b89d78",
                        "bc62872304756d30c0899c941767174faebfc406",
                        "3d6ef06ab51be5a86cdfaba246db2c2eb1d07795"
                    ],
                    "contains": [
                        "vault id"
                    ]
                },
                {
                    "data_path": "action_result.parameter.file_type",
                    "data_type": "string",
                    "example_values": [
                        "txt",
                        "docx"
                    ]
                },
                {
                    "data_path": "action_result.parameter.is_structured",
                    "data_type": "boolean"
                },
                {
                    "data_path": "action_result.parameter.max_artifacts",
                    "data_type": "numeric",
                    "example_values": [
                        "100"
                    ]
                },
                {
                    "data_path": "action_result.parameter.container_id",
                    "data_type": "numeric",
                    "example_values": [
                        "1776",
                        "52069"
                    ]
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1,
                        2
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1,
                        0
                    ]
                }
            ],
            "versions": "EQ(*)"
        }
    ]
}
