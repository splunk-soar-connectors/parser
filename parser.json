{
    "appid": "4004f489-1826-44a1-af12-9c74eea82eb0",
    "name": "Parser",
    "description": "This app extracts IOCs from various files such as PDFs, emails, or raw text",
    "type": "information",
    "product_vendor": "Phantom",
    "logo": "logo_phantom.svg",
    "logo_dark": "logo_phantom_dark.svg",
    "product_name": "Parser",
    "product_version_regex": ".*",
    "publisher": "Splunk",
